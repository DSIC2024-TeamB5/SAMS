<Window x:Class="WpfApplication1.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:WpfApplication1"
        mc:Ignorable="d"
        Title="MainWindow" Height="350" Width="525" Loaded="Window_Loaded" Closing="Window_Closing">
    <Grid>
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="449*"/>
            <ColumnDefinition Width="68*"/>
        </Grid.ColumnDefinitions>
        <ListBox x:Name="listBox" HorizontalAlignment="Left" Height="85" Margin="28,96,0,0" VerticalAlignment="Top" Width="411"/>
        <Button x:Name="btnCreateObj" Content="Create" HorizontalAlignment="Left" Margin="28,44,0,0" VerticalAlignment="Top" Width="70" Click="btnCreateObj_Click" Height="33"/>
        <Button x:Name="btnSetHandle" Content="SetHandle" HorizontalAlignment="Left" Margin="124,44,0,0" VerticalAlignment="Top" Width="70" Click="btnSetHadle_Click" Height="33"/>
        <Button x:Name="btnSendMsg" Content="SendMsg" HorizontalAlignment="Left" Margin="212,44,0,0" VerticalAlignment="Top" Width="85" Height="33" Click="btnSendMsg_Click"/>
        <Button x:Name="btnDeleteObj" Content="Delete" HorizontalAlignment="Left" Margin="315,44,0,0" VerticalAlignment="Top" Width="70" Height="33" Click="btnDeleteObj_Click"/>
        <Button x:Name="btnPlugIn" Content="PlugIn" HorizontalAlignment="Left" Height="30" Margin="44,224,0,0" VerticalAlignment="Top" Width="70" Click="btnPlugIn_Click" />
        <Button x:Name="btnPlugOut" Content="PlugOut" HorizontalAlignment="Left" Height="30" Margin="154,224,0,0" VerticalAlignment="Top" Width="70" Click="btnPlugOut_Click"/>

    </Grid>
</Window>
